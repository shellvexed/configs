!--------------------
!-[Xdefaults file]
!--------------------

!-[Font Rendering]-----
Xft.dpi:		      99
Xft.antialias:		true
Xft.rgba:         rgb
Xft.hinting:		true
Xft.hintstyle:		hintfull
Xft.lcdfilter:		lcddefault

!-[Fonts]----
!URxvt.font:xft:terminus:pixelsize=10:antialias=false
!URxvt.boldFont:xft:terminus:bold:pixelsize=10:antialias=false
!URxvt.font: xft:ProFontWindows:size=9:antialias=false
!URxvt.boldFont: xft:ProFontWindows:size=9:antialias=false
!URxvt.font:xft:ProggySmallTT:pixelsize=16
!URxvt.boldFont:xft:ProggySmallTT:pixelsize=16
!URxvt.font:xft:ProggyCleanTT:pixelsize=16
!URxvt.boldFont:xft:ProggyCleanTT:pixelsize=16
URxvt.font:xft:Envy Code R:medium:pixelsize=12
URxvt.boldFont:xft:Envy Code R:medium:pixelsize=12
!URxvt.font:xft:Consolas:style=Regular:pixelsize=14
!URxvt.boldFont:xft:Consolas:style=Regular:pixelsize=14

!-[URxvt Settings]----
URxvt.saveLines:	8192
URxvt.scrollBar:	false
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:	/usr/bin/jumanji
URxvt.matcher.button:	1
!URxvt.cursorColor:      #5E468C
URxvt.underlineColor:   yellow
URxvt.geometry:		80x36
!URxvt.transparent: 	true
URxvt.tintColor:	#000000
URxvt.shading:		60

!-[colors]----
URxvt.cursorColor: #DCDCCC

